---
import CtaFive from '../../components/ctas/CtaFive.astro';
import BaseLayout from '../../layouts/BaseLayout.astro';
import { AstroSeo } from '@astrolib/seo';
import Curriculum from '../../components/features/Curriculum.astro';
import Faqs from '../../components/Infopages/Faqs.astro';
---

<AstroSeo
  description="DXMentorship is a 12-week mentorship program for developers who want to start a career in developer advocacy."
  canonical="https://www.dxmentorship.com"
  openGraph={{
    url: 'https://www.dxmentorship.com',
    title: 'DXMentorship',
    description:
      'DXMentorship is a 12-week mentorship program for developers who want to start a career in developer advocacy.',
    images: [
      {
        url: 'https://github.com/Dxmentorship/dxmentorship/assets/66284362/2e0ac661-708b-483d-9018-2188a070e2a7',
        width: 1200,
        height: 630,
        alt: 'Social open graph',
        type: 'image/jpeg',
      },
      {
        url: 'https://github.com/Dxmentorship/dxmentorship/assets/66284362/2e0ac661-708b-483d-9018-2188a070e2a7',
        width: 4096,
        height: 4096,
        alt: 'Twitter open graph',
        type: 'image/jpeg',
      },
      { url: 'https://github.com/Dxmentorship/dxmentorship/assets/66284362/2e0ac661-708b-483d-9018-2188a070e2a7' },
      { url: 'https://github.com/Dxmentorship/dxmentorship/assets/66284362/2e0ac661-708b-483d-9018-2188a070e2a7' },
    ],
    site_name: 'DXMentorship',
  }}
  twitter={{
    handle: '@dxmentorship',
    site: '@site',
    cardType: 'summary_large_image',
  }}
/>
<BaseLayout>
  <section>
    <Faqs />
  </section>
</BaseLayout>
